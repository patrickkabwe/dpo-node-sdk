{
  "name": "@kazion/dpo-node-sdk",
  "version": "0.0.8",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "npm version patch && tsup",
    "postversion": "git push && git push --tags"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "outDir": "lib",
    "splitting": false,
    "sourcemap": false,
    "dts": true,
    "minify": true,
    "clean": true
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/node": "^18.15.11",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dpo",
    "dpo-sdk",
    "dpo-sdk-node",
    "dpo-sdk-nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kazion500/dpo-pay-sdk"
  }
}
