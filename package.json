{
  "name": "@kazion/dpopay-sdk",
  "version": "2.0.0-beta.1",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "push": "npm version minor && git push && git push --tags",
    "git:push": "git add -A && git commit -m 'new release' && git push",
    "audit:fix": "yarn-audit-fix --force"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "outDir": "lib",
    "splitting": false,
    "sourcemap": false,
    "dts": true,
    "minify": true,
    "clean": true
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/node": "^18.15.11",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^1.2.2",
    "yarn-audit-fix": "^10.0.7"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "fast-xml-parser": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dpo",
    "dpo-sdk",
    "dpo-sdk-node",
    "dpo-sdk-nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kazion500/dpo-pay-sdk"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
